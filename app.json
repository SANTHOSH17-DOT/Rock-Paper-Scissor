{
  "expo": {
    "name": "Rock Paper Scissor",
    "slug": "Rock-Paper-Scissor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/RPS.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/RPS.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/RPS.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.santhosh_m.RockPaperScissor"
    },
    "web": {
      "favicon": "./assets/RPS.png"
    }
  }
}